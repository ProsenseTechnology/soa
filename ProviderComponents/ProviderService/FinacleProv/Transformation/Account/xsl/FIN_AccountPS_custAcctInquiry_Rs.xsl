                    <xsl:value-of select="app:Body/app:executeFinacleScriptResponse/app:executeFinacleScript_CustomData/app:CustAcctInquiry_RES/app:AcctDtls/app:AccountName"/>
                  </com:FullName>
                  <com:FirstName>
                    <xsl:value-of select="app:Body/app:executeFinacleScriptResponse/app:executeFinacleScript_CustomData/app:CustAcctInquiry_RES/app:CustomerFirstName"/>
                  </com:FirstName>
                  <com:LastName>
                    <xsl:value-of select="app:Body/app:executeFinacleScriptResponse/app:executeFinacleScript_CustomData/app:CustAcctInquiry_RES/app:CustomerLastName"/>
                  </com:LastName>
                  <com:ShortName>
                    <xsl:value-of select="app:Body/app:executeFinacleScriptResponse/app:executeFinacleScript_CustomData/app:CustAcctInquiry_RES/app:CustomerShortName"/>
