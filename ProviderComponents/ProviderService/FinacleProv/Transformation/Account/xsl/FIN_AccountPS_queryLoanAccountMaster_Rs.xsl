            <eo:DisbursementAccount>
              <xsl:value-of select="app:DisbursementAccount"/>
            </eo:DisbursementAccount>
            <eo:InterestRepaymentAccount>
              <xsl:value-of select="app:InterestRepaymentAccount"/>
            </eo:InterestRepaymentAccount>
            <eo:Tenure>
              <xsl:value-of select="app:Tenure"/>
            </eo:Tenure>
            <eo:ApprovedLimit>
              <xsl:value-of select="app:ApprovedLimit"/>
            </eo:ApprovedLimit>
            <eo:NextPrincipalDueDate>
              <xsl:value-of select="app:NextPrincipalDueDate"/>
            </eo:NextPrincipalDueDate>
            <eo:NextInterestDueDate>
              <xsl:value-of select="app:NextInterestDueDate"/>
            </eo:NextInterestDueDate>
            <eo:NoofDelayed_Payments>
              <xsl:value-of select="app:NoofDelayedPayments"/>
            </eo:NoofDelayed_Payments>
            <eo:DisbursedAmount>
              <xsl:value-of select="app:DisbursedAmount"/>
            </eo:DisbursedAmount>
            <com:StartDate>
              <xsl:value-of select="app:LoanStartDate"/>
            </com:StartDate>
            <com:EndDate/>
            <com:TotalAmount>
              <xsl:value-of select="app:TotalLoanAmount"/>
            </com:TotalAmount>
            <eo:ClosureDate>
              <xsl:value-of select="app:LoanClosureDate"/>
            </eo:ClosureDate>
            <eo:AccountStatus>
              <xsl:value-of select="app:Loanstatus"/>
            </eo:AccountStatus>
          </eo:Account>
        </xsl:for-each>
        <xsl:if test="count (/app:QueryLoanAccountMasterOutputCollection/app:QueryLoanAccountMasterOutput ) > 0">
          <com:Status>
            <com:Code>ESB-00</com:Code>
            <com:Type>Success</com:Type>
            <com:Text>Loan account master details queried successfully</com:Text>
          </com:Status>
        </xsl:if>
