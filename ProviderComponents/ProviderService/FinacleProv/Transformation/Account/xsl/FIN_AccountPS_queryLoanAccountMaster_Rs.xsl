  <xsl:template match="/">
    <eo:ESBMsg>
      <eo:RsData>
        <xsl:for-each select="/app:QueryLoanAccountMasterOutputCollection/app:QueryLoanAccountMasterOutput">
          <eo:Account>
            <com:AccountIdentifier>
              <com:BankID>
                <xsl:value-of select="app:UnitId"/>
              </com:BankID>
              <com:AccountID>
                <xsl:value-of select="app:LoanAccountNumber"/>
              </com:AccountID>
              <com:AccountCurrency>
                <xsl:value-of select="app:LoanCurrency"/>
              </com:AccountCurrency>
            </com:AccountIdentifier>
            <eo:AccountType>
              <xsl:value-of select="app:AccType"/>
            </eo:AccountType>
            <eo:AccountSubType>
              <xsl:value-of select="app:AccSubType"/>
            </eo:AccountSubType>
            <custeo:Customer>
              <com:Person>
                <com:PersonName>
                  <com:FullName>
                    <xsl:value-of select="app:LoanAccountName"/>
                  </com:FullName>
                </com:PersonName>
              </com:Person>
            </custeo:Customer>
            <eo:DelinquencyStatus>
              <xsl:value-of select="app:DelinquencyStatus"/>
            </eo:DelinquencyStatus>
            <com:Amount currencyCode=""/>
