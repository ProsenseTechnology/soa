              <xsl:value-of select="app:LoanMaturityDate"/>
            </eo:MaturityDate>
            <eo:InterestRate>
              <xsl:value-of select="app:InterestRate"/>
            </eo:InterestRate>
            <eo:InterestAccrued>
              <xsl:value-of select="app:InterestAccrued"/>
            </eo:InterestAccrued>
            <eo:SettlementCurrency>
              <xsl:value-of select="app:SettlementCurrency"/>
            </eo:SettlementCurrency>
            <eo:LoanType>
              <xsl:value-of select="app:LoanType"/>
            </eo:LoanType>
            <eo:FundingAccount>
              <xsl:value-of select="app:FundingAccount"/>
            </eo:FundingAccount>
            <eo:PaymentFrequency>
              <xsl:value-of select="app:PaymentFrequency"/>
            </eo:PaymentFrequency>
            <eo:Original_Amount>
              <xsl:value-of select="app:OriginalAmount"/>
            </eo:Original_Amount>
            <eo:OutStanding_Amount>
              <xsl:value-of select="app:OutStandingAmount"/>
            </eo:OutStanding_Amount>
            <eo:PastDueAmount>
              <xsl:value-of select="app:PastDueAmount"/>
            </eo:PastDueAmount>
            <eo:LastPaymentAmount>
              <xsl:value-of select="app:LastPaymentAmount"/>
            </eo:LastPaymentAmount>
            <eo:LastPaymentReceivedDate>
              <xsl:value-of select="app:LastPaymentReceivedDate"/>
            </eo:LastPaymentReceivedDate>
            <eo:TotalNumberofInstallments>
              <xsl:value-of select="app:TotalNumberofInstallments"/>
