<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/FileNotification/V1"
            xmlns:com="http://xmlns.equitybank.co.ke/ESB/EO/Common/V1" xmlns:rcom="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/Common/V1"
            xmlns:eo="http://xmlns.equitybank.co.ke/ESB/EO/Misc/Notification/V1" targetNamespace="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/FileNotification/V1"
            elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xsd:import namespace="http://xmlns.equitybank.co.ke/ESB/EO/Common/V1" schemaLocation="../../../../EnterpriseObjects/Common/V1/CommonComponents.xsd"/>
    <xsd:import namespace="http://xmlns.equitybank.co.ke/ESB/EO/Misc/Notification/V1"
                schemaLocation="../../../../EnterpriseObjects/Misc/Notification/V1/NotificationEO.xsd"/>
    <xsd:import namespace="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/Common/V1" schemaLocation="../../Common/V1/CommonComponents.xsd"/>
    <!-- ====================================================================== -->
    <!-- ============= Referenced Elements ============= -->
    <!-- ====================================================================== -->
    <xsd:element name="FileNotification" type="FileNotificationType"/>
    <!-- ====================================================================== -->
    <!-- ============= Notification ============= -->
    <!-- ====================================================================== -->
    <xsd:complexType name="FileNotificationType">
        <xsd:sequence>
            <xsd:element name="TranId" type="com:StringType"/>
            <xsd:element name="ClientId" type="com:StringType"/>
            <xsd:element name="WSID" type="com:StringType"/>
            <xsd:element name="Type" type="com:StringType"/>
            <xsd:element name="FileIdentifier" type="com:StringType"/>
            <xsd:element name="FileName" type="com:StringType"/>
            <xsd:element name="FileLocation" type="com:StringType"/>
            <xsd:element name="ProcessStatus" type="com:StringType"/>
            <xsd:element name="ReferenceNumber" type="com:StringType"/>
            <xsd:element name="CrossReferenceNumber" type="com:StringType"/>
            <xsd:element name="UnitID" type="com:StringType"/>
        </xsd:sequence>
    </xsd:complexType>
    <!-- ====================================================================== -->
    <!-- ============= ESB Message ============= -->
    <!-- ====================================================================== -->
    <xsd:complexType name="ESBMsgType">
        <xsd:complexContent>
            <xsd:extension base="rcom:ESBMsgType">
                <xsd:sequence>
                    <xsd:choice>
                        <xsd:element name="RqData" type="RqDataType"/>
                        <xsd:element name="RsData" type="RsDataType"/>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ESBMsg" type="ESBMsgType"/>
    <xsd:complexType name="RqDataType">
        <xsd:sequence>
            <xsd:element ref="FileNotification" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="RsDataType">
        <xsd:sequence>
            <xsd:element ref="FileNotification" minOccurs="0"/>
            <xsd:element ref="com:Status" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>