<?xml version = '1.0' encoding = 'UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:rs="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/ChequeBook/V1"
             xmlns="http://wadl.dev.java.net/2009/02">
    <doc title="ChequeBookRS_rest">ChequeBookRS REST Service</doc>
    <grammars>
        <xsd:schema>
            <xsd:import namespace="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/ChequeBook/V1" schemaLocation="ChequeBookRO.xsd"/>
        </xsd:schema>
    </grammars>
    <resources>
        <resource path="/create">
            <method name="POST" soa:wsdlOperation="create">
                <request>
                    <representation mediaType="application/xml" element="rs:ESBMsg"/>
                </request>
                <response status="200">
                    <representation mediaType="application/xml" element="rs:ESBMsg"/>
                </response>
            </method>
        </resource>
        <resource path="/reserveSeries">
            <method name="POST" soa:wsdlOperation="reserveSeries">
                <request>
                    <representation mediaType="application/xml" element="rs:ESBMsg"/>
                </request>
                <response status="200">
                    <representation mediaType="application/xml" element="rs:ESBMsg"/>
                </response>
            </method>
        </resource>
    </resources>
</application>
