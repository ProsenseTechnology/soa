<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/Common/V1"
            xmlns:com="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/Common/V1" targetNamespace="http://xmlns.equitybank.co.ke/ESB/RS/Polaris/Common/V1"
            elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xsd:complexType name="AmountType">
        <xsd:sequence>
            <xsd:element name="Value" type="xsd:decimal" minOccurs="0"/>
            <xsd:element name="CurrencyCode" type="xsd:normalizedString" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="RateType">
        <xsd:sequence>
            <xsd:element name="Value" type="xsd:decimal" minOccurs="0"/>
            <xsd:element name="CurrencyCode" type="xsd:normalizedString" minOccurs="0"/>
            <xsd:element name="BaseValue" type="xsd:decimal" minOccurs="0"/>
            <xsd:element name="BaseCurrencyCode" type="xsd:normalizedString" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
